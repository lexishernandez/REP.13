% y=((x-(-2)))

% limpiar variables
 clear

% Ejemplo f (x) = a

% Rango de -3..3 en i = 0.1
 x = [-3: 1 :3]; 

% Valor de la funciÃ³n 
 y=((x-(+2)))

% Dibujar x, y
 plot(x , y);

% Titulo ("f (x)=((x-(+2)))

% Etiqueta para x
 xlabel ( "x");

% Etiqueta para y
ylabel ("y");
